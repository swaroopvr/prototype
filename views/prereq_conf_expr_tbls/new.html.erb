<% breadcrumb :new_preconf, [@base_cmds,@prereqs]%>
<h1>CREATE PREREQ_CONF_EXPR_TBL </h1>
<%= form_for(@prereqs) do |f| %>

  <%= f.label :CMD_ID %>
  <%= f.collection_select(:CMD_ID, BASE_CMD_TBL.all, :CMD_ID, :DESCRIPTION, {:prompt => "Please select"})%></br>

  <%= f.label :ENTRY_TYPE %>
  <%= f.text_field :ENTRY_TYPE, class: 'form-control' %></br>

  <%= f.label :FIELD_NO%>
  <%= f.text_field :FIELD_NO, class: 'form-control' %></br>

  <%= f.label :EXPR_ID %>
  <%= f.collection_select(:EXPR_ID, EXPRESSION.all, :EXPR_ID, :concat_three_fields, {:prompt => "Please select"})%><%=link_to "Doesn't exist?", new_expression_path%></br>

  <%= f.label :LOGICAL_OP%>
  <%= f.text_field :LOGICAL_OP, class: 'form-control' %></br>

  <%= f.submit 'Create', class: "btn btn-primary" %>
<% end %>
