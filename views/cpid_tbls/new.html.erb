
<h1>CREATE CPID_TBL </h1>
<%= form_for(@cpids) do |f| %>

  <%= f.label :CMD_ID %>
  <%= f.collection_select(:CMD_ID, BASE_CMD_TBL.all, :CMD_ID, :DESCRIPTION, {:prompt => "Please select"})%></br>

  <%= f.label :CPID %>
  <%= f.text_field :CPID, class: 'form-control' %>

  <%= f.label :CPID_DESC%>
  <%= f.text_field :CPID_DESC, class: 'form-control' %>

  <%= f.label :TM_VAL %>
  <%= f.text_field :TM_VAL, class: 'form-control' %>

  <%= f.label :TM_COUNT %>
  <%= f.text_field :TM_COUNT, class: 'form-control' %>

  <%= f.label :PROC_TYPE%>
  <%= f.text_field :PROC_TYPE, class: 'form-control' %>

  <%= f.submit 'Create', class: "btn btn-primary" %>
<% end %>
